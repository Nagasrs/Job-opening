   Finding Duplicate
Select Col 1, Col 2, Count(*)
From Table A
Group BY Col 1, Col 2
HAVING COUNT(*) > 1;
  Second Highest salary
Selct Max(Salary) As Secondhighestsalart
From Employee
Where Salary < (Select Max(Salary) from Employee);
  Employee without department
Select employee
From employee Table e
LEFT JOIN where Department D as e.Department_id = d.Department_id
Where d.Department_id is NULL;
  Total Revenue per product
Select Product_id SUM(Quantity*Price) AS total_revenue
From Sales
Group_By Product_id;
  Top 3 Highest paid
Select * From Employee
Order by salary DESC_LIMIT 3;
  All employees joined in 2025
Select * From Employee
Where Exract(Year From hire_date) = 2025;
   Average Order value per customer
Select Customer_id, AVG(total_amount) AS avg_order_value
From Orders
Group by Customer_id;
   Products never sold
Select p.product_id
From Product p
LEFT JOIN Sales s ON p.product_id = s.product_id
Where s.product_id IS NULL;
   Hired on weekend
Select* From employee
Where Extract(DOW from hire_date) IN (0,6);
   Salary between 50k and 1L
Select* From employee
Where Salary between 50000 and 100000;
  Rank Employee by salary
Select employee_id, department_id, salary;
Rank() OVER (PARTITION BY department_id order by Salary DESC() As Salary_rk
From employee;
  Customers who places orders every month in 2025
Select Customer_id;
From Orders
Where Extract (Year From order_date) = 2025
Group by customer_id
Having Count(Distinct To_CHAR(Order_date, 'yyyy-MM']] = 12;




